{
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/node",
      "config": {
        "distDir": "client/build"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "https://xpense-gqrg.onrender.com/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
